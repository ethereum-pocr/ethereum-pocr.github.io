        "babel-core": "^7.0.0-bridge.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^27.5.1",
        "eslint": "^6.7.2",
        "eslint-plugin-vue": "^6.2.2",
        "jest": "^27.0.5",
        "vue-cli-plugin-vuetify": "~2.4.2",
        "vue-jest": "^3.0.7",
        "vue-template-compiler": "^2.6.11"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "parserOptions": {
            "parser": "babel-eslint"
        },
        "rules": {},
        "ignorePatterns": [],
        "overrides": [
            {
                "files": [
                    "**/__tests__/*.{j,t}s?(x)",
                    "**/tests/unit/**/*.spec.{j,t}s?(x)"
                ],
                "env": {
                    "jest": true
                }
            }
        ]
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ],
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "vue"
        ],
        "transform": {
            ".*\\.(js)$": "babel-jest"
        },
        "preset": "@vue/cli-plugin-unit-jest",
        "setupFiles": [
            "<rootDir>/tests/unit/setup.js"
        ]
    }
}