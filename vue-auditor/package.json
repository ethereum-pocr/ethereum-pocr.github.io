        "ethereum-blockies": "github:ethereum/blockies",
        "ethers": "^5.6.2",
        "oidc-client": "^1.11.5",
        "roboto-fontface": "*",
        "vue": "^2.6.11",
        "vue-router": "^3.5.3",
        "vuetify": "^2.4.0",
        "vuex": "^3.6.2",
        "vuex-pathify": "^1.5.1",
        "vuex-persistedstate": "^4.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.17.7",
        "@babel/preset-env": "^7.16.11",
